---
const { width, height } = Astro.props;
---
<svg
    xmlns="http://www.w3.org/2000/svg"
    width={width || '24'}
    height={height || '24'}
    viewBox="0 0 512 512"
    fill="none"
    class="icon icon-ionic"
>
    <circle cx="256" cy="256" r="208" stroke="currentColor" stroke-width="32" stroke-miterlimit="10" fill="none"/>
    <circle cx="256" cy="256" r="80" fill="currentColor"/>
    <circle cx="416" cy="128" r="40" fill="currentColor" stroke="white" stroke-width="4"/>
    <ellipse cx="320" cy="96" rx="16" ry="8" fill="currentColor" opacity="0.4"/>
</svg>
